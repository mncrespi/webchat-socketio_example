{
  "name": "node_scaffolding",
  "version": "1.0.0",
  "description": "",
  "author": "Matias Crespi <crespimatiasnicolas@gmail.com>",
  "main": "index.js",
  "private": false,
  "engines": {
    "node": ">=6.x.x",
    "npm": ">=3.x.x"
  },
  "scripts": {
    "start": "gulp serve",
    "build": "gulp",
    "lint": "gulp lint",
    "test": "gulp mocha",
