  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mncrespi/node_scaffolding.git"
  },
  "keywords": [
    "express",
    "node",
    "node.js",
    "mongodb",
    "mongoose",
    "es6",
    "mocha",
